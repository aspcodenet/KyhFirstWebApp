@page
@model FirstWebApp.Pages.OrdersModel
@{
}

<table class="table">
    <tr>
        <td>Id</td>
        <td>Customer</td>
        <td>Datum</td>
    </tr>
    @foreach (var order in Model.Orders)
    {
        <tr>
            <td><a asp-route-id="@order.Id" asp-page="/OrderInfo">@order.Id</a></td>
            <td>@order.CustomerName</td>
            <td>@order.DateTime</td>
        </tr>
    }

</table>